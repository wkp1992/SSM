$(function(){
	$("#et").panel({
		title : '面板',
		iconCls : 'icon-save',
		width : 500,
		height : 450,
		border : true,
		fit : false,
		//content : '<div style="color:red;">我是面板内容</div>',
		collapsible : true,//定义是否显示可折叠按钮。
		minimizable : true,//定义是否显示最小化按钮。
		maximizable : true,//定义是否显示最大化按钮。
		closable : true,//定义是否显示关闭按钮。
		tools : '#panel-tools',
		href : 'panel-content.jsp',
	});
	
});

function open1(){
	$("#et").panel("open");
}

function close1(){
	$("#et").panel("close");
}





